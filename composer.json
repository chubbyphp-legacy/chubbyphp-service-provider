{
    "name": "chubbyphp/chubbyphp-service-provider",
    "description": "Chubbyphp service provider",
    "keywords": ["chubbyphp", "provider", "pimple"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dominik Zogg",
            "email": "dominik.zogg@gmail.com"
        }
    ],
    "require": {
        "php": "~7.0",
        "psr/log": "~1.0"
    },
    "conflict": {
        "monolog/monolog": "<1.4.1,>=2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5.8",
        "chubbyphp/chubbyphp-mock": "~1.0@dev",
        "alcaeus/mongo-php-adapter": "^1.1.5",
        "doctrine/mongodb": "~1.1",
        "monolog/monolog": "^1.4.1",
        "pimple/pimple": "^3.0",
        "symfony/yaml": "~2.7|~3.0|~4.0"
    },
    "provide" : {
      "ext-mongo": "*"
    },
    "autoload": {
        "psr-4": { "Chubbyphp\\ServiceProvider\\": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
